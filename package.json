{
  "name": "data-to-vis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cors-anywhere": "^0.4.0",
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-brush": "^1.0.4",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-time-format": "^2.1.0",
    "install": "^0.10.1",
    "node-sass-chokidar": "0.0.3",
    "npm": "^5.5.1",
    "npm-run-all": "^4.1.1",
    "object-assign": "^4.1.1",
    "promise-polyfill": "^6.0.2",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-datepicker": "^0.55.0",
    "react-dom": "^16.0.0",
    "react-faux-dom": "^4.0.5",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-cors": "node cors-anywhere.js",
    "start": "npm-run-all -p watch-css start-cors start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "react-test-renderer": "^16.0.0"
  }
